# CDCP Development Environment Variables
# このファイルは.gitignoreに含めないこと（開発環境のデフォルト値）

# Node Environment
NODE_ENV=development

# Database
DATABASE_URL="postgresql://cdcp_dev:cdcp_dev_password@localhost:5432/cdcp_development?schema=public"
DATABASE_TEST_URL="postgresql://cdcp_test:cdcp_test_password@localhost:5433/cdcp_test?schema=public"

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=cdcp_redis_password
REDIS_DB=0

# Auth0 (開発環境用 - 本番環境では.env.productionを使用)
AUTH0_DOMAIN=dev-cdcp.auth0.com
AUTH0_CLIENT_ID=your_dev_client_id
AUTH0_CLIENT_SECRET=your_dev_client_secret
AUTH0_AUDIENCE=https://api.cdcp.dev
AUTH0_CALLBACK_URL=http://localhost:3001/auth/callback
JWT_SECRET=dev_jwt_secret_change_in_production

# AWS S3 (Localstack)
AWS_REGION=ap-northeast-1
AWS_ACCESS_KEY_ID=test
AWS_SECRET_ACCESS_KEY=test
AWS_S3_BUCKET=cdcp-photos-dev
AWS_S3_ENDPOINT=http://localhost:4566
AWS_CLOUDFRONT_URL=http://localhost:4566

# API Configuration
API_PORT=3000
API_PREFIX=/api
API_VERSION=v1
API_RATE_LIMIT_TTL=60
API_RATE_LIMIT_MAX=60

# Frontend
FRONTEND_PORT=3001
FRONTEND_URL=http://localhost:3001
VITE_API_URL=http://localhost:3000/api/v1

# Logging
LOG_LEVEL=debug
LOG_FORMAT=pretty

# Monitoring (開発環境では無効化)
DATADOG_API_KEY=
SENTRY_DSN=

# File Upload
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/heic

# CORS
CORS_ORIGIN=http://localhost:3001
CORS_CREDENTIALS=true
